{
  "type": "stdio",
  "command": "node",
  "args": [
    "${ACF_PATH}/bin/agentic-control-framework-mcp",
    "--workspaceRoot",
    "${WORKSPACE_ROOT}"
  ],
  "env": {
    "ACF_PATH": "<absolute/path/to/agentic-control-framework>",
    "WORKSPACE_ROOT": "${cwd}",
    "READONLY_MODE": "false",
    "BROWSER_HEADLESS": "false",
    "DEFAULT_SHELL": "/bin/bash",
    "NODE_ENV": "production"
  }
}
