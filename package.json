{
  "name": "agentic-control-framework",
  "version": "0.1.1",
  "description": "CLI and MCP task manager with AI-based PRD parsing",
  "main": "src/core.js",
  "bin": {
    "acf": "./bin/task-manager",
    "acf-mcp": "./bin/agentic-control-framework-mcp",
    "task-manager": "./bin/task-manager",
    "task-manager-mcp": "./bin/task-manager-mcp",
    "agentic-control-framework-mcp": "./bin/agentic-control-framework-mcp"
  },
  "scripts": {
    "start:mcp": "node ./bin/agentic-control-framework-mcp",
    "start:legacy-mcp": "node ./bin/task-manager-mcp",
    "start": "node ./src/mcp_server.js --workspaceRoot $(pwd)",
    "start:auth": "node auth-proxy.js",
    "start:local": "docker-compose up --build",
    "test": "mocha 'test/**/*.js'"
  },
  "keywords": [
    "task",
    "manager",
    "cli",
    "mcp",
    "cursor",
    "agentic",
    "framework",
    "control",
    "monetization",
    "stripe",
    "auth"
  ],
  "author": "Abhilash Chadhar",
  "license": "ISC",
  "dependencies": {
    "@vscode/ripgrep": "^1.15.9",
    "axios": "^1.7.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "diff": "^5.2.0",
    "dotenv": "^16.5.0",
    "execa": "^5.1.1",
    "glob": "^10.3.10",
    "mime-types": "^2.1.35",
    "minimatch": "^9.0.3",
    "node-fetch": "^2.7.0",
    "playwright": "^1.52.0",
    "ps-list": "^8.1.1",
    "sharp": "^0.33.5",
    "tree-kill": "^1.2.2"
  },
  "optionalDependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "stripe": "^14.7.0"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "mocha": "^10.7.0"
  }
}
